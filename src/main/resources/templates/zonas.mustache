{{#layout}}
    <div class="container" id="zonas">
        <h1>Listado de Zonas</h1>
        <table>
            <thead>
            <tr>
                <th>Nombre</th>
                <th>Precio</th>
                <th>ID</th>
            </tr>
            </thead>
            <tbody>
            <tr>
                <td>Zona A</td>
                <td class="price">$100</td>
                <td>2</td>
            </tr>
            {{#zonas}}
                <tr>
                    <td>{{nombre}}</td>
                    <td class="price">{{precio}}</td>
                    <td>{{id}}</td>
                </tr>
            {{/zonas}}
            <!-- Puedes añadir más filas aquí -->
            </tbody>
        </table>
    </div>
{{/layout}}

{{>layout}}