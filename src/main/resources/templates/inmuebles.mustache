{{#layout}}
    <div class="container" id="inmuebles">
        <h1>Listado de Inmuebles</h1>
        <table>
            <tr>
                <th>Zona</th>
                <th>Cantidad de Ambientes</th>
                <th>Tamaño (m²)</th>
            </tr>
            </thead>
            <tbody>
            {{#inmuebles}}
                <tr>
                    <td>{{zona.nombre}}</td>
                    <td>{{cantAmbientes}}</td>
                    <td>{{tamanioEnM2}}</td>
                </tr>
            {{/inmuebles}}
            </tbody>
        </table>
    </div>
{{/layout}}

